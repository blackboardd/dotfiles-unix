ARM_COMMON_FLAGS="-static"
ARM_CC_AS_FLAGS="-g -mfpu=neon -march=armv8-a"
ARM_CXX_FLAGS="std=c++17"

function sapp() {
   cd $HOME'/Code/sponsorcx/app/'$1
}

function sapi() {
   cd $HOME'/Code/sponsorcx/api/'$1
}

function snat() {
   cd $HOME'/Code/sponsorcx/native/'$1
}

function ssty() {
   cd $HOME'/Code/sponsorcx/style/'$1
}

function sgor() {
   cd $HOME'/Code/sponsorcx/gorgeous/'$1
}

# automt automatically mounts all available drives
alias music='vlc --recursive expand /run/media/blackboardd/1A52C24752C226FB/music /home/blackboardd/Music /home/blackboardd/Soulseek\ Downloads &'
alias automt='sudo mount -a'
alias prapp='gh pr create -f -r creedmangrum -B staging'
alias prapi='gh pr create -f -r creedmangrum -B staging'
alias gimmekeys='xmodmap -e "clear lock" && setxkbmap -option ctrl:nocaps && setxkbmap -option caps:escape && xmodmap $HOME/.Xmodmap'
alias slackt="slack-term -config ~/.slack"
alias mutt="neomutt"
alias sl="ls"
alias gd="git branch -D"
alias gl="git log --pretty=oneline | nvim"
alias gr="git_return"
alias gs="git status"
alias ga="git add"
alias gwa="git worktree add"
alias gra="git worktree remove"
alias gp="git push"
alias gpu="git push -u origin"
alias gpl="git pull"
alias "cz commit"="ca"
alias  vi="nvim"
alias vim="nvim"
alias config='/usr/bin/git --git-dir=$HOME/.cfg/ --work-tree=$HOME'
alias clip="clip.exe"
alias cxds="sudo docker start $CX_CONTAINERsudo docker exec -it $CX_CONTAINER /bin/bash"
alias cxde="sudo docker exec -it $CX_CONTAINER /bin/bash"


